 <h1 id="title">Order Items</h1>
  <div class="outside">

  <!--Main Layout-->
  <main>

    <!-- Main Navigation-->
    
    <header>
      <div class="link">
        <a href="/">Home</a>
      </div>
      <div class="link">
        <a href="">Manage Inventory</a>
      </div>
      <div class="link">
        <a href="order.handlebars">Manage Orders</a>
      </div>
      <div class="link">
        <a href="customer.handlebars">Manage Customers</a>
      </div>
    </header>

    <section class="tableWrapper">
      <h2>Current Order Items</h2>

        <h3>Customer Name</h3>
        <p hidden>{{customer_Num}}</p>
        <p>{{first_Name}} {{last_Name}}</p>

        <h3>Order Date</h3>
        <p>{{oDate}}</p>

      <table>
         <tr class="tableHeader">
          <th>Item</th>
          <th>Item Type</th>
          <th>Quantity</th>
          <th>Selling Price</th>
          <th>Shipped</th>
          <th>Shipping Date</th>
        </tr>
        <tbody>
          {{#each items}}
          <tr>
            <td>{{item_Name}}</td>
            <td>{{item_Price}}</td>
            <td>{{number_Items_In_Stock}}</td>
            <th>
              <button onclick="editItem({{item_Num}})">Edit</button>
              <button onclick="deleteItem({{item_Num}})">Delete</button>
            </th>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </section>
  </div>
  </main>
